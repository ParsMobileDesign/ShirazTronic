@model List<CompanyInfos>
@{ 
    string email = "Email is not provided";
    string tel = "Telephone is not provided";
}
@if (Model!=null && Model.Count>0)
{
    var companyinfo = Model[0];
    email = companyinfo.EMail;
    tel = companyinfo.TelNo;
}
    <section id="topbar" class="d-none d-lg-block">
        <div class="container d-flex">
            <div class="contact-info mr-auto">
                <i class="icofont-envelope"></i><a href="@email">@email</a>
                <i class="icofont-phone"></i> @tel
            </div>
            <div class="social-links">
                <a href="#" class="twitter"><i class='bx bxl-twitter'></i></a>
                <a href="#" class="facebook"><i class='bx bxl-facebook'></i></a>
                <a href="#" class="instagram"><i class='bx bxl-instagram'></i></a>
                <a href="#" class="skype"><i class='bx bxl-skype'></i></a>
                <a href="#" class="linkedin"><i class='bx bxl-linkedin-square'></i></a>
            </div>
        </div>
    </section>