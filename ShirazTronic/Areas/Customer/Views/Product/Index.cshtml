
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@{string[] itemTitle = { "Encoders", "I/O Modules", "Industrial Automation", "Motors Drives", "Printers" }; }



<section id="product-list" class="product-list section-bg">
    <div class="container">

        <div class="section-title aos-init aos-animate" data-aos="fade-up">
            <h2>list of Products</></h2>
            <p>you can find a wide range of @itemTitle[2] here</p>
        </div>

        <div class="row">
            @for (int i = 0; i < 12; i++)
            {
                string title = "/image/tempProducts/" + (i + 1).ToString() + ".jpg";

                <div class="col-lg-3 col-md-6 d-flex align-items-stretch">
                    <div class="product-list-item aos-init aos-animate" data-aos="fade-up">
                        <div class="product-list-item-img">
                            <img src=@title class="img-fluid" alt="">
                            <div class="extra-link">
                                <a href="">
                                    <i class='bx bxs-cart-add'></i>
                                </a>
                                <a asp-action="SelProduct">
                                    <i class='bx bx-show-alt'></i>
                                </a>
                            </div>
                        </div>
                        <div class="product-list-item-info">
                            <h4>@itemTitle[2]</h4>
                            <span>Class AA . useful for borads</span>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</section>








